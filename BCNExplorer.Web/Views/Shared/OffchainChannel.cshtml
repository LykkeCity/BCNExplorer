@model OffchainChannelViewModel 
@{
    Layout = null;
}


@if (Model.CloseTransaction != null)
{
    
    @Html.Partial("TransactionDetails", Model.CloseTransaction)
}

@foreach (var tx in Model.OffChainTransactions)
{
    @Html.Partial("OffchainTransactionDetails", tx)
}

@if(Model.OpenTransaction != null)
{
    @Html.Partial("TransactionDetails", Model.OpenTransaction)
}

